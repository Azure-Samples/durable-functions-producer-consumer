            },
            {
              "name": "DFTaskHubName",
              "value": "producerTaskHub"
            },
            {
              "name": "EventHubConnection",
              "value": "[listkeys(variables('ehAuthRuleResourceId'), '2017-04-01').primaryConnectionString]"
            },
            {
              "name": "EventHubName",
              "value": "sample"
            },
            {
              "name": "EventHubKafkaConnection",
              "value": "[listkeys(variables('ehkAuthRuleResourceId'), '2017-04-01').primaryConnectionString]"
            },
            {
              "name": "EventHubKafkaName",
              "value": "sample"
            },
            {
              "name": "EventHubKafkaFQDN",
              "value": "[concat(parameters('EventHubKafkaNamespaceName'), '.servicebus.windows.net:9093')]"
            },
            {
              "name": "EventHubKafkaPartitions",
              "value": "32"
            },
            {
              "name": "EventHubPartitions",
              "value": "32"
            },
            {
              "name": "EventGridTopicEndpoint",
              "value": "[reference(parameters('EventGridTopicName')).endpoint]"
            },
            {
              "name": "EventGridTopicKey",
